{
    "manifest_version": 3,
    "name": "AI Form Filler",
    "version": "1.0",
    "description": "Upload personal data, save to database, and auto-fill forms!",
    "permissions": ["storage", "scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "assets/logo.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  